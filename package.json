{
  "name": "typescript-starter",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "set NODE_ENV=development && ts-node-dev --respawn src/index.ts",
    "prod": "set NODE_ENV=production && ts-node-dev --respawn src/index.ts",
    "debug": "export NODE_ENV=development & ndb ts-node-dev src/index.ts",
    "build": "npm run clean && tsc -b",
    "clean": "rimraf ./dist"
  },
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.4.8",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.36"
  }
}
